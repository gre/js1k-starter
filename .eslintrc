{
  "rules": {
    "no-unused-vars": 0,
    "no-sparse-arrays": 0,
    "indent": [
      2,
      2
    ],
    "quotes": [
      2,
      "double"
    ],
    "linebreak-style": [
      2,
      "unix"
    ],
    "semi": [
      2,
      "always"
    ]
  },
  "env": {
    "browser": true
  },
  "extends": "eslint:recommended"
}
